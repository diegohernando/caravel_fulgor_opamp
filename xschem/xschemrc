#Configure xschem project directory to use sky130 symbols
set XSCHEM_LIBRARY_PATH {}
append XSCHEM_LIBRARY_PATH :${XSCHEM_SHAREDIR}/xschem_library
append XSCHEM_LIBRARY_PATH :~/skywater/xschem_sky130
append XSCHEM_LIBRARY_PATH :~/caravel_fulgor_opamp/xschem/opamp_diego
append XSCHEM_LIBRARY_PATH :~/caravel_fulgor_opamp/xschem/opamp_julia
append XSCHEM_LIBRARY_PATH :~/caravel_fulgor_opamp/xschem/ring_vco

